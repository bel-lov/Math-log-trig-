{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "no-underscore-dangle": "off",
        "no-tabs": 0,
        "no-eval": 0,
        "import/prefer-default-export": "off",
        "no-restricted-syntax": [
			"error",
			"LabeledStatement",
			"WithStatement"
		]
    }
        
}
